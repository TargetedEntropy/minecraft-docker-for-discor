{
  "vanilla": {
    "name": "Vanilla Minecraft",
    "description": "Standard Minecraft server without mods",
    "image": "itzg/minecraft-server:latest",
    "environment": {
      "EULA": "TRUE",
      "TYPE": "VANILLA",
      "MEMORY": "2G",
      "DIFFICULTY": "normal",
      "MAX_PLAYERS": "20",
      "MOTD": "A Vanilla Minecraft Server",
      "SPAWN_PROTECTION": "16"
    },
    "ports": {
      "25565/tcp": null
    },
    "volumes": {},
    "restart_policy": {
      "Name": "unless-stopped"
    }
  },
